{
  "name": "brekekejs",
  "version": "2107.2.1",
  "private": true,
  "scripts": {
    "format": "prettier --ignore-path=.gitignore --loglevel=silent --write \"**/*.{js,json,md}\""
  },
  "dependencies": {
    "url-parse": "1.5.1"
  },
  "devDependencies": {
    "prettier": "2.3.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
  }
}
